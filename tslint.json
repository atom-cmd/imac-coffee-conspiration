{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "no-console": false,
		"indent": [true, "tabs"],
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
		"object-literal-sort-keys": false,
		"ordered-imports": false
    },
    "rulesDirectory": []
}